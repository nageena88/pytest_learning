{
    "python.analysis.completeFunctionParens": true,
    "python.testing.pytestArgs": [
        "tests",
        "--no-cov",
        "-v"
    ],
    "cSpell.words": [
        "aedtresults",
        "aedtz",
        "archiver",
        "Astropy",
        "atitude",
        "autofetch",
        "autouse",
        "aview",
        "brosesdm",
        "Chauhan",
        "commitizen",
        "CUST",
        "dataclass",
        "datefmt",
        "datetime",
        "DEMAG",
        "dymola",
        "EMAG",
        "Fahrzeugteile",
        "fncs",
        "hdlr",
        "initializable",
        "isclass",
        "levelname",
        "levelno",
        "LOGFILE",
        "matlabsimulink",
        "msvcrt",
        "phid",
        "Popen",
        "PREONLAB",
        "prscence",
        "prscene",
        "Pylance",
        "pyproject",
        "pytest",
        "rglob",
        "sdmcurve",
        "SDMDL",
        "sdminfo",
        "sdmlock",
        "sdmmeta",
        "sdmrs",
        "SDMS",
        "SDMTK",
        "simulationx",
        "SIMULINK",
        "strtobool",
        "submodel",
        "submodels",
        "tkui",
        "tqdm",
        "wbpj",
        "wbpz"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "autoDocstring.docstringFormat": "google",
    // python formatter: black
    "python.formatting.provider": "black",
    // python tests: pytest
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.envFile": "${workspaceFolder}/.env",
    // files will be exclueded from vscode explorer
    "files.exclude": {
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/*.py,cover": true,
        "starter.py": true,
        "XmlImportWithSTL": true
    },
    "python.analysis.indexing": true,
}